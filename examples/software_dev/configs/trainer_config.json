    "optimizer_config_path": "configs/optimizer_config.json",

    "optim_order": "order",
    "optimizers": [
        "prompt",
        "node"
    ],
    "additional_info": {
        "loss": "",
        "sop": "",
        "node": "",
        "prompt": ""
    }
}