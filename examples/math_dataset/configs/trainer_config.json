    "use_roll_back": true,

    "parallel": true,
    "parallel_max_num": 8,

    "initial_solution_path": "examples/math_dataset/configs/solution.json",
    "optimizer_config_path": "examples/math_dataset/configs/optimizer_config.json",
    "optim_order": "order",
    "optimizers": [
        "prompt"
    ],
    "additional_info": {
        "loss": "",
        "sop": "",
        "node": "",
        "prompt": ""
    }
